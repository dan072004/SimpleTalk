      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ST-KafkaConsumer
      target: base
    container_name: consumerapi
    depends_on:
      producer:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: dan07consumerapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/netcoreapp3.1/ST-KafkaConsumer.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 8003
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Apps\Kafka\SimpleTalk\ST-KafkaProducer\ST-KafkaConsumer
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\daniel.moldovan\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\daniel.moldovan\AppData\Roaming/ASP.NET/Https
